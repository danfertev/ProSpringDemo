<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="targetByName" class="ru.anfdenis.prospring3.ch4.autowiring.Target" autowire="byName" lazy-init="true"/>

    <bean id="targetByType" class="ru.anfdenis.prospring3.ch4.autowiring.Target" autowire="byType" lazy-init="true"/>

    <bean id="targetConstructor" class="ru.anfdenis.prospring3.ch4.autowiring.Target" autowire="constructor"
          lazy-init="true"/>

    <bean id="foo" class="ru.anfdenis.prospring3.ch4.autowiring.Foo"/>

    <bean id="bar" class="ru.anfdenis.prospring3.ch4.autowiring.Bar"/>

</beans>